translate(\"Yes\")}</Button>\r\n                    <Button onClick={handleConfirmClose} autoFocus>\r\n                        {translate(\"No\")}\r\n                    </Button>\r\n                </DialogActions>\r\n            </Dialog>\r\n        </div>\r\n    );\r\n\r\n    /************************** Handle Delete User ****************************/\r\n    const tableRef = useRef();\r\n    const renderUsers = () => {\r\n        if (filteredUsers != null && filteredUsers.length > 0) {\r\n            let data = filteredUsers;\r\n            let options = {\r\n                // exportAllData: true,\r\n                // exportButton: true,\r\n                actionsColumnIndex: -1,\r\n                headerStyle: {\r\n                    backgroundColor: \"#01579b\",\r\n                    color: \"#FFF\",\r\n                    fontWeight: \"bold\",\r\n                },\r\n                filtering: false,\r\n                paging: true,\r\n                pageSize: 10,\r\n                maxBodyHeight: \"500px\",\r\n                search: false,\r\n                tableLayout: \"auto\",\r\n            };\r\n            let actions = [\r\n                {\r\n                    icon: () => (\r\n                        <SuiButton\r\n                            style={{\r\n                                minWidth: 80,\r\n                                color: \"#dc3545\",\r\n                                backgroundColor: \"transparent\",\r\n                                border: \"1px solid #dc3545\",\r\n                            }}\r\n                            color={\"danger\"}\r\n                        >\r\n                            {translate(\"Delete\")}\r\n                        </SuiButton>\r\n                    ),\r\n                    onClick: (evt, data) => handleConfirmOpen(data),\r\n                },\r\n                {\r\n                    icon: () => (\r\n                        <SuiButton style={{ minWidth: 140, width: 140 }} color={\"primary\"}>\r\n                            {translate(\"تفاصيل\")}\r\n                            <FileCopy />\r\n                        </SuiButton>\r\n                    ),\r\n                    onClick: (evt, data) => getUser(data),\r\n                },\r\n            ];\r\n            if (!loading)\r\n                return (\r\n                    <div ref={tableRef}>\r\n                        <ActionTable\r\n                            Title={translate(\"جدول المستخدمين\")}\r\n                            Columns={columns}\r\n                            Data={data.map(item => {\r\n                                return {\r\n                                    ...item,\r\n                                    roles: item.roles.map(e => e.name).join(\" , \")\r\n                                }\r\n                            })}\r\n                            Options={options}\r\n                            Actions={actions}\r\n                        />\r\n                    </div>\r\n                );\r\n            else\r\n                return <Placeholder loading={loading} />;\r\n        } else return <Placeholder loading={false} />;\r\n    };\r\n\r\n    const handleBack = () => {\r\n        getUsers();\r\n        setshowUserDetail(false)\r\n    }\r\n    return (\r\n        <GridContainer>\r\n            {!showUserDetail && (\r\n                <>\r\n                    <GridItem md={12}>\r\n                        <GridItem container md={12} style={{ margin: \"0px 0px 10px 0\" }}>\r\n                            <GridItem>\r\n                                <Button\r\n                                    style={{ margin: \"0px 5px\" }}\r\n                                    disabled={false}\r\n                                    variant=\"contained\"\r\n                                    className={classes.submitBtn}\r\n                                    onClick={handleCreate}\r\n                                >\r\n                                    <span style={{ padding: \"0px 0px 0px 10px\" }}>\r\n                                        {translate(\"إضافة مستخدم\")}\r\n                                    </span>\r\n                                    <AddBox />\r\n                                </Button>\r\n                            </GridItem>\r\n                        </GridItem>\r\n                    </GridItem>\r\n                    <GridItem md={12}>{renderUsers()}</GridItem>\r\n                </>\r\n            )}\r\n            {showUserDetail && (\r\n                <UserDetails\r\n                    isEditable={isEditable}\r\n                    isCreate={isCreate}\r\n                    details={User}\r\n                    activateEdit={activateEdit}\r\n                    handleBack={handleBack}\r\n                    show={showUserDetail}\r\n                />\r\n            )}\r\n            <ConfirmDialog />\r\n        </GridContainer>\r\n    );\r\n};\r\n(UsersList as any).auth = true;\r\n(UsersList as any).layout = Admin;\r\nexport default UsersList;\r\n"]},"metadata":{},"sourceType":"module"}