<Card style={fileItem}>\r\n                  <div style={fileContainer}>\r\n                    <a\r\n                      style={fileLink}\r\n                      href={`${process.env.BASE_URL}/${item.url}`}\r\n                      target={\"_blank\"}\r\n                    >\r\n                      {item.name}\r\n                    </a>\r\n                    <IconButton\r\n                      style={icon}\r\n                      onClick={() => submitDeleteAttachment({ id: item.id })}\r\n                    >\r\n                      <Delete style={icon} />\r\n                    </IconButton>\r\n                  </div>\r\n                </Card>\r\n              </GridItem>\r\n            </GridContainer>\r\n          ))}\r\n        </React.Fragment>\r\n      );\r\n    } else return <Placeholder />;\r\n  };\r\n  return (\r\n    <React.Fragment>\r\n      <Card>\r\n        <CardHeader>\r\n          <h4 style={{ fontWeight: \"bold\", color: \"#01579b\" }}>\r\n            {translate(\"Attachments\")}\r\n          </h4>\r\n        </CardHeader>\r\n        <CardBody>\r\n          <GridContainer>\r\n            <GridItem md={12}>\r\n              <Grid container>\r\n                <Grid md={6}>\r\n                  {renderUploadButton(\r\n                    2,\r\n                    translate(\"Upload personal image\"),\r\n                    \"personal_image\"\r\n                  )}\r\n                </Grid>\r\n                <Grid md={6}>\r\n                  {renderUploadButton(\r\n                    1,\r\n                    translate(\"Upload ID image\"),\r\n                    \"id_image\"\r\n                  )}\r\n                </Grid>\r\n              </Grid>\r\n              <div id={\"certificate\"} style={spacer} />\r\n              {renderUploadButton(\r\n                3,\r\n                translate(\"Upload Other Attachment\"),\r\n                \"other_attachment\"\r\n              )}\r\n              <div id={\"certificate\"} style={