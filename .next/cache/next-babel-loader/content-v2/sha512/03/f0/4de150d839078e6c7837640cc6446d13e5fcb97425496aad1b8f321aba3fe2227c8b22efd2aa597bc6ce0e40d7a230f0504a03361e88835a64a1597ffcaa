ll && filteredData.length > 0) {\r\n      let options = {\r\n        // exportAllData: true,\r\n        // exportButton: true,\r\n        actionsColumnIndex: -1,\r\n        headerStyle: {\r\n          backgroundColor: \"#01579b\",\r\n          color: \"#FFF\",\r\n          fontWeight: \"bold\",\r\n        },\r\n        filtering: false,\r\n        paging: true,\r\n        pageSize: 10,\r\n        maxBodyHeight: \"500px\",\r\n        search: false,\r\n        tableLayout: \"auto\",\r\n      };\r\n      let actions = [\r\n        {\r\n          icon: () => (\r\n            <SuiButton style={{ minWidth: 140, width: 140 }} color={\"primary\"}>\r\n              {translate(\"Details\")}\r\n              <FileCopy />\r\n            </SuiButton>\r\n          ),\r\n          onClick: (evt, data) => handleCourseDetail(data),\r\n        },\r\n        {\r\n          icon: () => (\r\n            <SuiButton\r\n              style={{\r\n                minWidth: 80,\r\n                color: \"#dc3545\",\r\n                backgroundColor: \"transparent\",\r\n                border: \"1px solid #dc3545\",\r\n              }}\r\n              color={\"danger\"}\r\n            >\r\n              {translate(\"Delete\")}\r\n            </SuiButton>\r\n          ),\r\n          onClick: (evt, data) => handleConfirmDialog(data),\r\n        },\r\n      ];\r\n      if (type == \"teachers\") {\r\n        actions.splice(1, 0, {\r\n          icon: () => (\r\n            <SuiButton style={{ minWidth: 140, width: 140 }} color={\"primary\"}>\r\n              {translate(\"Assign Teacher\")}\r\n              <Assignment />\r\n            </SuiButton>\r\n          ),\r\n          onClick: (evt, data) => Assign(data),\r\n        });\r\n      }\r\n      return (\r\n        <div ref={tableRef}>\r\n          <ActionTable\r\n            Title={`${translate(\"All\")} ${data.length}`}\r\n            Columns={columns}\r\n            Data={data}\r\n            Options={options}\r\n            Actions={actions}\r\n          />\r\n        </div>\r\n      );\r\n    } else return <Placeholder loading={false} />;\r\n  };\r\n  const ConfirmDialog = ({ id, show, handleClose, handleDeleteCourse }) => {\r\n    return (\r\n      <Dialog open={show} maxWidth=\"sm\" fullWidth>\r\n        <DialogTitle>\r\n          <Typography>{`هل تريد تأكيد حذف المقرر ${name}`}</Typography>\r\n        </DialogTitle>\r\n        <Box position=\"absolute\" top={0} right={0}>\r\n          <IconButton onClick={handleClose}>\r\n            <Close />\r\n          </IconButton>\r\n        </Box>\r\n        <DialogActions>\r\n          <Button onClick={handleClose} color=\"secondary\" variant=\"contained\">\r\n            {translate(\"Cancel\")}\r\n          </Button>\r\n          <Button\r\n            onClick={() => handleDeleteCourse(id)}\r\n            color=\"primary\"\r\n            variant=\"contained\"\r\n          >\r\n            {translate(\"Confirm\")}\r\n          </Button>\r\n        </DialogActions>\r\n      </Dialog>\r\n    );\r\n  };\r\n\r\n  const [showAssignTeacher, setShowAssignTeacher] = useState(false);\r\n  const [title, setTitle] = useState(\"\");\r\n  const [pdata, setPdata] = useState(null);\r\n  const Assign = (data) => {\r\n    console.clear();\r\n    console.log(data);\r\n    const _title = `تكليف مدرّس ضمن المقرّر: \r\n    (${data?.edu_course?.course?.code}) \r\n    (${data?.edu_course?.course?.ar_name}) \r\n    (${data?.edu_year?.year}) \r\n    (${data?.year?.speciality?.ar_name})\r\n    (${data?.year?.ar_name})\r\n    `;\r\n    setTitle(_title);\r\n    setPdata(data);\r\n    setShowAssignTeacher(true);\r\n  };\r\n  const setHide = () => {\r\n    setShowAssignTeacher(!showAssignTeacher);\r\n  };\r\n  return (\r\n    <GridContainer>\r\n      {!showAssignTeacher && (\r\n        <>\r\n          <GridItem md={12}>\r\n            <GridItem container md={12} style={{ margin: \"0px 0px 10px 0\" }}>\r\n              <GridItem md={7}></GridItem>\r\n              <GridItem></GridIte