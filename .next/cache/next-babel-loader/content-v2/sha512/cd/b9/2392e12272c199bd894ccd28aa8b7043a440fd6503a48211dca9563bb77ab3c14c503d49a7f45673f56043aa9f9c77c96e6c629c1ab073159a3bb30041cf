\"\r\n                      name=\"file\"\r\n                      type=\"file\"\r\n                      ref={hiddenInput}\r\n                      style={{ display: \"none\" }}\r\n                      onChange={(event) => {\r\n                        setFieldValue(\"file\", event.currentTarget.files[0]);\r\n                      }}\r\n                      className=\"form-control\"\r\n                    />\r\n                    <Grid md={6} style={{ marginTop: \"1em\" }}>\r\n                      <TextField\r\n                        variant=\"outlined\"\r\n                        type=\"text\"\r\n                        size=\"small\"\r\n                        disabled\r\n                        placeholder=\"\"\r\n                        value={values.file?.name}\r\n                      />\r\n                    </Grid>\r\n                  </Box>\r\n                  <Box mt={4} mb={1}>\r\n                    {isEditable ? (\r\n                      isSubmitting ? (\r\n                        <SuiButton\r\n                          disabled={true}\r\n                          variant=\"gradient\"\r\n                          color=\"info\"\r\n                          fullWidth\r\n                        >\r\n                          {translate(\"Processing ...\")}\r\n                        </SuiButton>\r\n                      ) : (\r\n                        <SuiButton\r\n                          // disabled={!(dirty && isValid)}\r\n                          type=\"submit\"\r\n                          variant=\"gradient\"\r\n                          color=\"info\"\r\n                          fullWidth\r\n                        >\r\n                          {translate(\"Save\")}\r\n                        </SuiButton>\r\n                      )\r\n                    ) : (\r\n                      <SuiButton\r\n                        onClick={activateEdit}\r\n                        type=\"submit\"\r\n                        variant=\"gradient\"\r\n                        color=\"info\"\r\n                        fullWidth\r\n                      >\r\n                        {translate(\"Edit Course\")}\r\n                      </SuiButton>\r\n                    )}\r\n                  </Box>\r\n                </Form>\r\n              );\r\n            }}\r\n          </Formik>\r\n        </Card>\r\n        <CardActions></CardActions>\r\n      </Grid>\r\n    </Grid>\r\n  );\r\n};\r\n\r\n(CourseDetail as any).layout = Admin;\r\n(CourseDetail as any).auth = false;\r\nconst mapStateToProps = (state) => {\r\n  console.log(\"state\", state);\r\n  return {\r\n    candidate: state.baseReducer.course,\r\n  };\r\n};\r\n\r\nexport default connect(mapStateToProps, null)(CourseDetail);\r\n"]},"metadata":{},"sourceType":"module"}