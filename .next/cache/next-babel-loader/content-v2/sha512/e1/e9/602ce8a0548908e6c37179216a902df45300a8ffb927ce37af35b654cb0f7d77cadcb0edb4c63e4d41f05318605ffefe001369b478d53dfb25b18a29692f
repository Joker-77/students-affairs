      },\r\n        {\r\n          title: translate(\"French Name\"),\r\n          field: \"fr_name\",\r\n        },\r\n        {\r\n          title: translate(\"Course Code\"),\r\n          field: \"code\",\r\n        },\r\n        {\r\n          title: translate(\"Total Hours\"),\r\n          field: \"current_description.total_hours\",\r\n        },\r\n        {\r\n          title: translate(\"Credit Hours\"),\r\n          field: \"current_description.credit\",\r\n        },\r\n      ];\r\n      let data = filteredCourses;\r\n      let options = {\r\n        // exportAllData: true,\r\n        // exportButton: true,\r\n        actionsColumnIndex: -1,\r\n        headerStyle: {\r\n          backgroundColor: \"#01579b\",\r\n          color: \"#FFF\",\r\n          fontWeight: \"bold\",\r\n        },\r\n        filtering: false,\r\n        paging: true,\r\n        pageSize: 10,\r\n        maxBodyHeight: \"500px\",\r\n        search: false,\r\n        tableLayout: \"auto\",\r\n      };\r\n      let actions = [\r\n        {\r\n          icon: () => (\r\n            <SuiButton\r\n              style={{\r\n                minWidth: 80,\r\n                color: \"#dc3545\",\r\n                backgroundColor: \"transparent\",\r\n                border: \"1px solid #dc3545\",\r\n              }}\r\n              color={\"danger\"}\r\n            >\r\n              {translate(\"Delete\")}\r\n            </SuiButton>\r\n          ),\r\n          onClick: (evt, data) => {},\r\n        },\r\n        {\r\n          icon: () => (\r\n            <SuiButton style={{ minWidth: 140, width: 140 }} color={\"primary\"}>\r\n              {translate(\"Course Details\")}\r\n            </SuiButton>\r\n          ),\r\n          onClick: (evt, data) => getCourse(data),\r\n        },\r\n      ];\r\n      return (\r\n        <ActionTable\r\n          Title={translate(\"Courses List\")}\r\n          Columns={columns}\r\n          Data={data}\r\n          Options={options}\r\n          Actions={actions}\r\n        />\r\n      );\r\n    } else return <Placeholder loading={false} />;\r\n  };\r\n  return (\r\n    <GridContainer>\r\n      {!showCourseDetail && (\r\n        <>\r\n          <GridItem md={12}>\r\n            <GridItem container md={12} style={{ margin: \"0px 0px 10px 0\" }}>\r\n              <GridItem md={7}>\r\n                {/* <Button\r\n                  style={{ margin: \"0px 5px\" }}\r\n                  disabled={false}\r\n                  variant=\"contained\"\r\n                  className={classes.submitBtn}\r\n                  onClick={handleOpen}\r\n                >\r\n                  <span style={{ padding: \"0px 0px 0px 10px\" }}>\r\n                    {translate(\"Add New Course\")}\r\n                  </span>\r\n                  <Add />\r\n                </Button> */}\r\n              </GridItem>\r\n              <GridItem>\r\n                <Button\r\n                  style={{ margin: \"0px 5px\" }}\r\n                  disabled={false}\r\n                  variant=\"contained\"\r\n                  className={classes.submitBtn}\r\n                >\r\n                  <span style={{ padding: \"0px 0px 0px 10px\" }}>\r\n                    {translate(\"Print\")}\r\n                  </span>\r\n                  <Print />\r\n                </Button>\r\n                <Button\r\n                  style={{ margin: \"0px 5px\" }}\r\n                  disabled={false}\r\n                  variant=\"contained\"\r\n                  className={classes.submitBtn}\r\n                >\r\n                  <span style={{ padding: \"0px 0px 0px 10px\" }}>\r\n                    {translate(\"Export to excel\")}\r\n                  </span>\r\n                  <Description />\r\n                </Button>\r\n                <Button\r\n                  style={{ margin: \"0px 5px\" }}\r\n                  disabled={false}\r\n                  variant=\"contained\"\r\n                  className={classes.submitBtn}\r\n                  onClick={(isCreate) => handleOpen(isCreate)}\r\n                >\r\n                  <span style={{ padding: \"0px 0px 0px 10px\" }}>\r\n                    {translate(\"Add Course Document\")}\r\n                  </span>\r\n                  <AddBox />\r\n                </Button>\r\n              </GridItem>\r\n            </GridItem>\r\n            <GridItem style={{ marginBottom: \"1em\", marginTop: \"2em\" }}>\r\n              <FormControl\r\n                size=\"small\"\r\n                variant=\"outlined\"\r\n                style={{ minWidth: 150, margin: \"0 0 0 1em\" }}\r\n              >\r\n                <InputLabel\r\n                  style={{ display: \"flex\" }}\r\n                  shrink\r\n                  ref={inputLabel}\r\n                  htmlFor=\"outlined-filter\"\r\n                >\r\n                  <span>{translate(\"Filter\")}</span>\r\n                  <FilterList />\r\n                </InputLabel>\r\n                <Select\r\n                  id=\"select-filter\"\r\n                  value={filter}\r\n                  onChange={handleChangeFilter}\r\n                  label={filters[filter].label}\r\n                  input={\r\n                    <OutlinedInput\r\n                      notched\r\n                      labelWidth={labelWidth}\r\n                      name=\"filter\"\r\n                      id=\"outlined-filter\"\r\n                    />\r\n                  }\r\n                >\r\n                  {filters.map((option) => (\r\n                    <MenuItem key={option.value} value={option.value}>\r\n                      {option.label}\r\n                    </MenuItem>\r\n                  ))}\r\n                </Select>\r\n              </FormControl>\r\n              <FormControl>\r\n                <TextField\r\n                  onChange={handleSearch}\r\n                  size=\"small\"\r\n                  id=\"outlined-basic\"\r\n                  label=\"\"\r\n                  variant=\"outlined\"\r\n                  InputProps={{\r\n                    startAdornment: (\r\n                      <InputAdornment position=\"start\">\r\n                        <Search />\r\n                      </InputAdornment>\r\n                    ),\r\n                  }}\r\n                ></TextField>\r\n              </FormControl>\r\n              <FormControl\r\n                size=\"small\"\r\n                variant=\"outlined\"\r\n                style={{ minWidth: 150, margin: \"0 1em 0 1em\" }}\r\n              >\r\n                <InputLabel\r\n                  style={{ display: \"flex\" }}\r\n                  shrink\r\n                  ref={inputSortLabel}\r\n                  htmlFor=\"outlined-sort\"\r\n                >\r\n                  <span>{translate(\"Sort By\")}</span>\r\n                  <Sort />\r\n                </InputLabel>\r\n                <Select\r\n                  displayEmpty\r\n                  fullWidth\r\n                  labelId=\"autowidth-label\"\r\n                  id=\"select-sort\"\r\n                  value={sortBy}\r\n                  onChange={handleSortBy}\r\n                  autoWidth\r\n                  input={\r\n                    <OutlinedInput\r\n                      notched\r\n                      labelWidth={sortLabelWidth}\r\n                      name=\"sort\"\r\n                      id=\"outlined-sort\"\r\n                    />\r\n                  }\r\n                  label={filters[sortBy].label}\r\n                >\r\n                  {filters.map((option) => (\r\n                    <MenuItem key={option.value} value={option.value}>\r\n                      {option.label}\r\n                    </MenuItem>\r\n                  ))}\r\n                </Select>\r\n              </FormControl>\r\n            </GridItem>\r\n          </GridItem>\r\n          <GridItem md={12}>{renderCourses()}</GridItem>\r\n        </>\r\n      )}\r\n      {showCourseDetail && (\r\n        <CourseDetails\r\n          details={course}\r\n          activateEdit={activateEdit}\r\n          setShow={setShow}\r\n          show={showCourseDetail}\r\n          isEditable={isEditable}\r\n        />\r\n      )}\r\n    </GridContainer>\r\n  );\r\n};\r\n(CoursesList as any).auth = true;\r\n(CoursesList as any).layout = Admin;\r\nexport default CoursesList;\r\n"]},"metadata":{},"sourceType":"module"}